      <text>Insecticide Exposure Triggers a Modulated Expression of ABC Transporter Genes in Larvae of Anopheles gambiae s.s.</text>
      <annotation id="1">
        <infon key="identifier"></infon>
        <infon key="type">Gene</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="56" length="15"/>
        <text>ABC Transporter</text>
      </annotation>
      <annotation id="2">
        <infon key="identifier"></infon>
        <infon key="type">Species</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="91" length="17"/>
        <text>Anopheles gambiae</text>
      </annotation>
