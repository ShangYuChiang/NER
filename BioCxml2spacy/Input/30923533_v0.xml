      <text>RNA Interference in the Tobacco Hornworm, Manduca sexta, Using Plastid-Encoded Long Double-Stranded RNA.</text>
      <annotation id="1">
        <infon key="identifier">7130</infon>
        <infon key="type">Species</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="24" length="16"/>
        <text>Tobacco Hornworm</text>
      </annotation>
      <annotation id="2">
        <infon key="identifier">7130</infon>
        <infon key="type">Species</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="42" length="13"/>
        <text>Manduca sexta</text>
      </annotation>
