      <text>Identification and Expression Profiling of Odorant Receptor Protein Genes in Sitophilus zeamais (Coleoptera: Curculionoidea) Using RT-qPCR.</text>
      <annotation id="27">
        <infon key="type">Gene</infon>
        <infon key="identifier"></infon>
        <infon key="annotator">5ef4bd7e0113</infon>
        <infon key="updated_at">2020-05-27T19:54:52Z</infon>
        <location offset="43" length="16"/>
        <text>Odorant Receptor</text>
      </annotation>
      <annotation id="2">
        <infon key="identifier">7047</infon>
        <infon key="type">Species</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
