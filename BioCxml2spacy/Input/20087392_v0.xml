      <text>The cys-loop ligand-gated ion channel gene superfamily of the parasitoid wasp, Nasonia vitripennis.</text>
      <annotation id="1">
        <infon key="identifier"></infon>
        <infon key="type">Gene</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="4" length="33"/>
        <text>cys-loop ligand-gated ion channel</text>
      </annotation>
      <annotation id="2">
        <infon key="identifier">7425</infon>
        <infon key="type">Species</infon>
        <infon key="updated_at">1980-01-01T00:00:00Z</infon>
        <location offset="79" length="19"/>
        <text>Nasonia vitripennis</text>
      </annotation>
